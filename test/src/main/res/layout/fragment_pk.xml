<?xml version="1.0" encoding="utf-8"?>
<org.jxxy.debug.corekit.widget.RoundConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="240dp"
    android:layout_height="300dp"
    app:round_corner="5dp"
    android:paddingLeft="50dp"
    android:paddingRight="50dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <org.jxxy.debug.corekit.widget.RoundConstraintLayout
        android:layout_width="240dp"
        android:layout_height="300dp"
        android:background="#f7f7f7"
        app:round_corner="5dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"/>

    <org.jxxy.debug.corekit.widget.IconFont
        android:id="@+id/pkFinishIcon"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:textSize="200sp"
        android:layout_marginTop="10dp"
        android:text="&#x100ad;"
        android:textColor="#FFD700"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"/>

    <TextView
        android:id="@+id/pkFinishTv"
        android:layout_width="200dp"
        android:layout_height="60dp"
        android:textSize="@dimen/title_text_size"
        android:textStyle="bold"
        android:textColor="#000000"
        android:gravity="center"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pkFinishIcon"/>

</org.jxxy.debug.corekit.widget.RoundConstraintLayout>